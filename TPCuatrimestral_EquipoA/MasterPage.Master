<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="MasterPage.master.cs" Inherits="TPCuatrimestral_EquipoA.MasterPage" %>

<!DOCTYPE html>
<html>
<head runat="server">
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>INMOBILIARIA LUSIVI</title>
    <link rel="stylesheet" href="~/css/Estilos.css" />
    <link rel="stylesheet" href="~/css/site.css" />

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    <script src="https://kit.fontawesome.com/341afd86c8.js" crossorigin="anonymous"></script>
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css" rel="stylesheet">
  
    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
    <style>
        .navbar {
            font-family: "Roboto", sans-serif;
            font-style: normal;
        }

        .navbar-blue {
            background-color: #4c6444;
            color: white;
        }

            .navbar-blue .nav-link {
                color: rgb(206, 206, 206);
                display: inline-flex; 
                justify-content: center;
                align-items: center;
                position: relative;
                font-size: 20px;
                text-decoration: none;
                padding: 0;
                background: none; 
                border: none; 
                margin-left: 10px;
            }

        .nav-link::after {
            content: '';
            position: absolute;
            left: 0;
            bottom: 0;
            width: 95%;
            height: 2.75px;
            background: white;
            border-radius: 100px;
            margin-left: 2px;
            transform: scaleX(0);
            transform-origin: right;
            transition: transform 0.2s;
        }

        .nav-link:hover {
            color: white;
        }

            .nav-link:hover::after {
                transform: scaleX(1);
                transform-origin: left;
            }

        .navbar-blue .navbar-brand {
            color: white;
        }

        .nav-link-button-wrapper {
            display: inline-flex; /* Cambiado de flex a inline-flex */
            justify-content: center;
            align-items: center;
            position: relative;
            font-size: 20px;
            text-decoration: none;
            padding: 0;
            background: none; /* Asegura que el fondo sea transparente */
            border: none; /* Asegura que no haya bordes */
            margin-left: 10px; /* Mantén el margen izquierdo para los botones */
        }

            .nav-link-button-wrapper::after {
                content: '';
                position: absolute;
                left: 0;
                bottom: -10px; /* Ajusta según sea necesario */
                width: 95%;
                height: 2.75px;
                background: white;
                border-radius: 100px;
                margin-left: 2px;
                transform: scaleX(0);
                transform-origin: right;
                transition: transform 0.2s;
            }

            .nav-link-button-wrapper:hover::after {
                transform: scaleX(1);
                transform-origin: left;
            }

        .nav-link-button {
            background: none;
            border: none;
            color: rgb(206, 206, 206); /* O el color que prefieras */
            font-size: 20px; /* Asegúrate de que coincida con el estilo de tus enlaces */
        }

            .nav-link-button:hover {
                color: white; /* O el color que prefieras para el hover */
            }


        body {
            background-color: white;
            font-family: "Roboto", sans-serif;
        }

        .navbar {
            background-color: #4c6444;
            font-family: "Roboto", sans-serif;
        }

        .navbar-brand img {
            margin-top: 5%;
            height: 140px;
        }

        .contacto-nav {
            font-family: "Roboto", sans-serif;
            font-style: normal;
            font-weight: bold;
            background-color: #caba9c;
            color: white;
            height: 30px;
            width: 100%;
            display: flex;
            justify-content: center;
            align-items: center;
        }

        .contacto-group {
            display: flex;
            gap: 25px;
            align-items: center;
            color: #4d2d18;
        }

        .nav-item {
            font-size: 20px;
            color: #caba9c;
        }

        .Redes_sociales {
            font-family: "Roboto", sans-serif;
            font-style: normal;
            font-size: 15px;
            color: white;
        }

        .autores {
            font-family: 'Baskerville Old Face';
            font-weight: bold;
            color: darkgrey;
            font-size: 15px;
        }

        footer {
            margin-block-end: 0px;
        }

        .btn-outline {
            color: #caba9c;
            font-size: 25px;
        }
    </style>
</head>
<body>
