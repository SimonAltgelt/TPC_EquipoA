                                    </div>
                                </li>
                                <li class="nav-item">
                                    <div class="nav-link-button-wrapper">
                                        <asp:Button ID="btnContacto" runat="server" CssClass="nav-link-button" Text="Contacto" OnClick="btnContacto_Click" />
                                    </div>
                                </li>
            
                            </ul>
                            <ul class="navbar-nav ms-auto">
                                 <li class="nav-item">
                                    <a href="InicioSesion.aspx" class="btn btn-outline">Iniciar sesión</a>
                                 </li>
                            </ul>
                        </div>
                    </div>
                </nav>
               <!-- contacto -->
